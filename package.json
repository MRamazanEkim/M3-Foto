{
  "name": "M3_Foto",
  "version": "1.0.0",
  "description": "QR kod ile foto yükleme ve slideshow gösterimi uygulaması",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm start\""
  },
  "keywords": [
    "electron",
    "photo",
    "slideshow",
    "qr-code"
  ],
  "author": "",
  "license": "MIT",
  "build": {
    "appId": "com.m3.foto",
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "app_icon.ico"
    },
    "asar": true
  },

  
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12"
  }
}
